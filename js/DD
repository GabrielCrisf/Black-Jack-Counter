//Cria o botao Butt
const Butt = document.createElement('button')
Butt.classList.add('Butte');
SEC_1.appendChild(Butt)

const Actionen = (evento) =>{
    const T = `Tecla pressed: ${evento.key}`
    console.log(`${evento.key}`)
    alert(T);
}

const Act = () =>{
    console.log('PQP');
}

Butt.addEventListener('keypress', () => {Actionen(event);});
Butt.addEventListener('auxclick', Act);
Butt.classList.add('Namen');

let e =  document.createElement('div')
Butt.appendChild (e);
Butt.textContent = 'CLIKA'

